</head>

<body>

  <nav class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
      <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <a class="navbar-brand" href="#"></a>
      </div>
      <div id="navbar" class="navbar-collapse collapse">
        <form class="navbar-form navbar-right">
          <!-- <a href="/games/contestant/#{<%= @contestant_id%>}"><button>Back to Game</button></a> -->
          <a href="/session/delete"><button type="submit" class="logout">Log Out</button></a>
        </div><!--/.navbar-collapse -->
      </div>
    </nav>

    <div class="jumbotron">
      <div class="container">

        <h1>Jeopardy Leaderboard</h1>
        <p>See how your skills stack up against other trivia nerds!</p>
      </div>
    </div>

    <div class="container">
      <div class="row">
       
        <div class="col-md-2">
          <h2>Ranking</h2>
        </div>

        <div class="col-md-3">
          <h2>Player</h2>
        </div>

        <div class="col-md-6">
          <h2>High Score</h2>
        </div>

     </div>


    <div class="container">
    <% @game.each do |game| %>
      <div class="row">

        <div class="col-md-2">
         <%# count = 0 %>
         <%# 10.times do %>
         <%# count += 1 %>
         <%# count %>
          <%# end %>

        </div>

        <div class="col-md-3">
          <h3><%= game.username %></h3>
        </div>

        <div class="col-md-6">
          <h3><%= game.game_score%></h3>
        </div>
        
     </div>

    <% end %>




  </div>
</div> 

<hr>

<footer>
  <p>&copy; Company 2014</p>
</footer>
</div> <!-- /container -->


