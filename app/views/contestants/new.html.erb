    <div class="container">
<div class="row">
      <div class="col-md-4">
  <form id="signup_form" method="POST" action="/contestants">
  
      <label for="exampleInputName">Name</label>
      <input id="name_form" type="text" name="name" class="form-control" id="inputName" placeholder="Enter name" required>
  
      <label for="exampleInputUsername">Username</label>
      <input id="username_form" type="text"  name="username" class="form-control" id="inputUsername" placeholder="Enter username" required>
  	
      <label for="exampleInputEmail">Email address</label>
      <input id="email_form" type="email" name="email" class="form-control" id="inputEmail" placeholder="Enter email">

      <label for="exampleInputPassword">Password</label>
      <input id="password_form" type="password" name="password" class="form-control" id="exampleInputPassword1" placeholder="Password" required>


      <!-- <label for="exampleInputPassword">Total Score</label>
